<%= form_for post do |f| %>
<%= f.hidden_field :lock_version%>
  <div class="form-group">
    <%= f.label :url%>
    <%= f.text_field :url, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :headline%>
    <%= f.text_field :headline, class: 'form-control'%>
  </div>

  <div class="form-group">
    <%= f.label :description %>
    <%= f.text_area :description, class: 'form-control' %>
  </div>

  <%= f.submit nil, class: 'btn btn-primary'%>
<%end%>
